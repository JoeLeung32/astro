---
import AppLayout from "../layouts/AppLayout/AppLayout.astro";
export const prerender = true;
const time = new Date().toLocaleTimeString();
---
<AppLayout>
    <div slot="header"/>
    <main class="container mx-auto p-4">
        <h1>Astro</h1>
        <h1>{time}</h1>
    </main>
    <div slot="footer"/>
</AppLayout>
