---
import AppLayout from "../../../layouts/AppLayout/AppLayout.astro";
const params = Astro.params
const time = new Date().toLocaleTimeString();
---
<AppLayout>
    <div slot="header"/>
    <main>
        <h1>/test/[type]/[id]</h1>
        <h1>{time}</h1>
        <h1>{JSON.stringify(params)}</h1>
    </main>
    <div slot="footer"/>
</AppLayout>